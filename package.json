{
  "name": "gitbook-plugin-git-author",
  "description": "show git author for each article of gitbook",
  "main": "index.js",
  "version": "1.1.3",
  "author": "L3au <leshu.lau@gmail.com>",
  "engines": {
    "node": ">4.0.0",
    "gitbook": ">3.0.0"
  },
  "scripts": {
    "lint": "xo --fix",
    "test": "npm run lint && nyc ava",
    "precommit": "npm run lint",
    "prepare": "npm run lint"
  },
  "files": [
    "assets"
  ],
  "keywords": [
    "gitbook",
    "plugin",
    "gitbook-plugin",
    "git",
    "author",
    "signature",
    "modify",
    "creator"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L3au/gitbook-plugin-git-author.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/L3au/gitbook-plugin-git-author/issues"
  },
  "dependencies": {
    "execa": "^0.6.3",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "husky": "^0.13.3",
    "nyc": "^10.2.0",
    "sinon": "^2.1.0",
    "xo": "^0.18.1"
  },
  "xo": {
    "space": true,
    "semicolon": false
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "html",
      "text",
      "text-summary"
    ]
  }
}
